    @if (ViewBag.TypeUsre == "Applicant")
    {
        <script>
            var StatusArabic, ApplicantId, JobId;
            $.ajax({
              url: '@Url.Action("ApplicantNotification", "Notifications")',
              contentType: "application/json; charset=utf-8",
              dataType: "json",
              Cache: false,
                success: function (Data) {
                    var NotNum = Data.length;
                    $(".NotificationsNum").html(NotNum);
                    $.each(Data, function (index) {
                        ApplicantId = Data[index].applicantId;
                        JobId = Data[index].jobId;
                      if (Data[index].applicationStatus == "Approved") {StatusArabic="قبول"} else{StatusArabic = "رفض"}
                        $(".NotificationMenu")
                            .append(`<li><a href = "Notifications/NotificateDetailsApplicant?ApplicantId=${ApplicantId}&JobId=${JobId}"> تم ` + StatusArabic + " طلبك في وظيفة "
                          + Data[index].jobName + "<br/>" + " بتارخ " + Data[index].progressTime.substr(0, 10) +'</a></li><hr/>');
                    });
                }
          });
        </script>
    }
    else if (ViewBag.TypeUsre == "OrgnisationAdmin")
    {
        <script>
            var StatusArabic;
            $.ajax({
              url: '@Url.Action("OrgnizationAdminNotification", "Notifications")',
              contentType: "application/json; charset=utf-8",
              dataType: "json",
              Cache: false,
                success: function (Data) {
                    var NotNum = Data.length;
                    $(".NotificationsNum").html(NotNum);
                    $.each(Data, function (index) {
                      if (Data[index].notificationType == 2) {
                          $(".NotificationMenu")
                              .append(`<li><a href="Notifications/NotificateDetailsOrgApplicant?ApplicantId=${Data[index].userSubjectId}&JobId=${Data[index].jobId}">تقدم ` + Data[index].userSubject + " لوظيفة "
                              + Data[index].jobName + "<br/>" + " بتارخ " + Data[index].progressTime.substr(0, 10) + '</a></li><hr/>');
                      }
                      else
                      {
                          if (Data[index].applicationStatus == "Approved") { StatusArabic = "قبول" } else { StatusArabic = "رفض" }
                          $(".NotificationMenu")
                              .append(`<li><a href="Notifications/NotificateOrgJobEval?JobId=${Data[index].jobId}">تم ` + StatusArabic + " طلبك في نشر وظيفة "
                              + Data[index].jobName + "<br/>" + " بتارخ " + Data[index].progressTime.substr(0, 10) + '</a></li><hr/>');
                      }
                  });
              }
          });
        </script>
    }
    else
    {
        <script>
            $.ajax({
              url: '@Url.Action("AdminNotificate", "Notifications")',
              contentType: "application/json; charset=utf-8",
              dataType: "json",
              Cache: false,
                success: function (Data) {
                    var NotNum = Data.length;
