    @*Code To Show PopUp Screen When Delete Any Item*@
<script>
        function showDetails(hrefElement) {
            var ActionName = hrefElement.getAttribute("data-ActionName");
            var ControllerName = hrefElement.getAttribute("data-ControllerName");
            var IdValue = hrefElement.getAttribute("data-IdValue");
            var UserIdApplicant = hrefElement.getAttribute("data-UserId");
            var OrderIdApplicant = hrefElement.getAttribute("data-OrderId");
            $("#Confirm").modal("show");
            $(".ExitPopUp").click(function () {
                $("#Confirm").modal("hide");
            });
            $(".ConfirmDelete").click(function () {
                switch (ControllerName) {
                    case "HiringOrder":
                        if (ActionName == "Delete") {
                            $.ajax({
                                url: '@Url.Action("Delete", "HiringOrder")',
                                method: "POST",
                                data: { Id: IdValue},
                                success: function () {
                                    location.reload();
                                }
                            });
                        }
                        else {
                                $.ajax({
                                url: '@Url.Action("DeleteHiringOrderApplicant", "HiringOrder")',
                                method: "POST",
                                data: { UserId: UserIdApplicant, OrderId: OrderIdApplicant},
                                success: function () {
                                    location.reload();
                                }
                            });
                        }
                        break;
                    case "Orgnization":
                         $.ajax({
                                url: '@Url.Action("Delete", "Orgnization")',
                                method: "POST",
                                data: { Id: IdValue},
                                success: function () {
                                    location.reload();
                                }
                            });
                        break;
                    case "Role":
                         $.ajax({
                                url: '@Url.Action("Delete", "Role")',
                                method: "POST",
                                data: { Id: IdValue},
                                success: function () {
                                    location.reload();
                                }
                             });
                        break;
                    case "User":
                         $.ajax({
                                url: '@Url.Action("Delete", "User")',
                                method: "POST",
                                data: { Id: IdValue},
                                success: function () {
                                    location.reload();
                                }
                            });
                        break;
                    default:

                }
            });
        };
</script>

